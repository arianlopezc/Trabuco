{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "version": "1.0",
  "lastUpdated": null,
  "project": {
    "name": "{{.ProjectName}}",
    "groupId": "{{.GroupID}}",
    "modules": [{{range $i, $m := .Modules}}{{if $i}}, {{end}}"{{$m}}"{{end}}],
    "database": {{if .HasModule "SQLDatastore"}}"{{.Database}}"{{else}}null{{end}},
    "noSqlDatabase": {{if .HasModule "NoSQLDatastore"}}"{{.NoSQLDatabase}}"{{else}}null{{end}},
    "messageBroker": {{if .HasModule "EventConsumer"}}"{{.MessageBroker}}"{{else}}null{{end}}
  },
  "git": {
    "branch": "",
    "uncommittedFiles": [],
    "lastCommitMessage": ""
  },
  "workInProgress": {
    "description": "",
    "startedAt": null,
    "completedSteps": [],
    "pendingSteps": [],
    "blockers": []
  },
  "testStatus": {
    "lastRun": null,
    "passed": 0,
    "failed": 0,
    "skipped": 0,
    "failedTests": []
  },
  "decisions": [],
  "notes": []
